{
  "extends": [
    "config:base",
    ":timezone(Asia/Tokyo)",
    ":prHourlyLimitNone",
    "github>whitesource/merge-confidence:beta",
    "github>aquaproj/aqua-renovate-config:file(.config/aqua/aqua.yaml)"
  ],
  "requiredStatusChecks": null,
  "schedule": [
    "every 1 hour after 00:00 and before 23:59 every day"
  ],
  "automergeSchedule": [
    "every 1 hour after 00:00 and before 23:59 every day"
  ],
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "automerge": true
    }
  ],
  "enabledManagers": [
    "regex"
  ]
}
