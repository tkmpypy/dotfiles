{
  "extends": [
    "config:base",
    ":timezone(Asia/Tokyo)",
    ":prHourlyLimitNone",
    "github>whitesource/merge-confidence:beta",
    "github>aquaproj/aqua-renovate-config:file(.config/aqua/aqua.yaml)"
  ],
  "schedule": [
    "before 9am"
  ],
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch"
      ],
      "matchCurrentVersion": "!/^0/",
      "automerge": true
    }
  ],
  "enabledManagers": [
    "regex"
  ]
}
