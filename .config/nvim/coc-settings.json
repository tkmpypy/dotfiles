{
  "coc.preferences.enableMarkdown": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.source.file.ignoreHidden": false,
  "coc.preferences.floatActions": true,
  "suggest.virtualText": true,
  "suggest.enablePreselect": false,
  "suggest.autoTrigger": "always",
  "suggest.triggerAfterInsertEnter": true,
  "suggest.snippetsSupport": true,
  "suggest.minTriggerInputLength": 1,
  "suggest.maxCompleteItemCount": 50,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "■ ",
  "signature.target": "float",
  "hover.target": "float",
  "floatFactory.floatConfig": {
    "border": true,
    "rounded": true,
    "winblend": 10
  },
  "codeLens.enable": true,
  "snippets.ultisnips.enable": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.dictionaries": ["dotfiles", "user"],
  "cSpell.dictionaryDefinitions": [
    {
      "name": "dotfiles",
      "path": "/Users/tkmpypy/.config/nvim/cspell/dotfiles.txt",
      "description": "dotfiles dictionary"
    },
    {
      "name": "user",
      "path": "/Users/tkmpypy/.local/share/nvim/cspell/user.txt",
      "description": "User local dictionary"
    }
  ],
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "sh": "shellcheck",
    "yaml": ["yamllint"],
    "cmake": ["cmake-lint", "cmakelint"],
    "typescript": "eslint",
    "javascript": "eslint",
    "typescriptreact": "eslint",
    "javascriptreact": "eslint",
    "jsx": "eslint",
    "tsx": "eslint",
    "go": "golangci-lint"
  },
  "diagnostic-languageserver.formatFiletypes": {
    "dart": "dartfmt",
    "python": ["black", "isort", "autopep8"],
    "lua": "stylua",
    "sh": "shfmt",
    "typescript": "prettier",
    "javascript": "prettier",
    "typescriptreact": "prettier",
    "javascriptreact": "prettier",
    "jsx": "prettier",
    "tsx": "prettier",
    "go": "gofumpt"
  },
  "diagnostic-languageserver.debug": true,
  "diagnostic-languageserver.formatters": {
    "stylua": {
      "command": "stylua",
      "args": ["--stdin-filepath", "%filepath", "--", "-"],
      "rootPatterns": [".stylua.toml"]
    },
    "prettier": {
      "command": "./node_modules/.bin/prettier",
      "args": ["--stdin-filepath", "%filepath"],
      "rootPatterns": [
        ".prettierrc",
        ".prettierrc.json",
        ".prettierrc.toml",
        ".prettierrc.json",
        ".prettierrc.yml",
        ".prettierrc.yaml",
        ".prettierrc.json5",
        ".prettierrc.js",
        ".prettierrc.cjs",
        "prettier.config.js",
        "prettier.config.cjs"
      ]
    },
    "autopep8": {
      "command": "autopep8",
      "args": ["-"]
    },
    "isort": {
      "command": "isort",
      "args": ["--quiet", "-"]
    }
  },
  "diagnostic-languageserver.linters": {
    "golangci-lint": {
      "command": "golangci-lint",
      "rootPatterns": [".git", "go.mod"],
      "debounce": 100,
      "args": ["run", "--out-format", "json"],
      "sourceName": "golangci-lint",
      "parseJson": {
        "sourceName": "Pos.Filename",
        "sourceNameFilter": true,
        "errorsRoot": "Issues",
        "line": "Pos.Line",
        "column": "Pos.Column",
        "message": "${Text} [${FromLinter}]"
      }
    },
    "eslint": {
      "command": "./node_modules/.bin/eslint",
      "rootPatterns": [".git"],
      "debounce": 100,
      "args": ["--stdin", "--stdin-filename", "%filepath", "--format", "json"],
      "sourceName": "eslint",
      "parseJson": {
        "errorsRoot": "[0].messages",
        "line": "line",
        "column": "column",
        "endLine": "endLine",
        "endColumn": "endColumn",
        "message": "${message} [${ruleId}]",
        "security": "severity"
      },
      "securities": {
        "2": "error",
        "1": "warning"
      }
    }
  },
  "sumneko-lua.enableNvimLuaDev": true,
  "json.schemas": [
    {
      "url": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json",
      "fileMatch": ["/*"]
    }
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json": "/*",
    "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.9.json": [
      "/docker-compose.yml"
    ]
  }
}
