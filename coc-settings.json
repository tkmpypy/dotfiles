{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "html.filetypes": ["html", "handlebars", "html.handlebars"],
  "html.format.enable": true,
  "python.jediEnabled": false,
  "python.formatting.provider": "yapf",
  "eslint.autoFixOnSave": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
  "flutter.provider.hot-reload": true,
  "translator.toLang": "ja",
  "translator.engines": ["bing", "google"],
  "coc.preferences.formatOnSaveFiletypes": ["python", "go", "json"],
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "•",
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "codeLens.enable": true
}
