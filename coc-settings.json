{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "html.filetypes": ["html", "handlebars", "html.handlebars"],
  "html.format.enable": true,
  "python.jediEnabled": false,
  "python.formatting.provider": "autopep8",
  "eslint.autoFixOnSave": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
  "flutter.provider.hot-reload": true,
  "translator.toLang": "ja",
  "translator.engines": ["bing", "google"],
  "coc.preferences.formatOnSaveFiletypes": ["python", "go", "json", "css"],
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.refreshAfterSave": true,
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "•",
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "codeLens.enable": false,
  "yaml.validate": true,
  "yaml.completion": true,
  "yaml.format.enable": true,
  "yaml.schemas": {
    "kubernetes": "/*.y*ml"
  }
}
