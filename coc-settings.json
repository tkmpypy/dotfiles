{
  "coc.preferences.currentFunctionSymbolAutoUpdate": true,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.enableMarkdown": true,
  "coc.preferences.snippets.enable": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "html.filetypes": ["html", "handlebars", "html.handlebars"],
  "html.format.enable": false,
  "python.formatting.provider": "autopep8",
  "eslint.autoFixOnSave": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
  "flutter.provider.hot-reload": true,
  "translator.toLang": "ja",
  "translator.engines": ["bing", "google"],
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.infoSign": "כֿ",
  "diagnostic.hintSign": "•",
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "◆ ",
  "codeLens.enable": true,
  "yaml.validate": false,
  "yaml.completion": true,
  "yaml.format.enable": true,
  "Lua.workspace.library": {
    "/usr/local/share/nvim/runtime/lua": true,
    "/usr/local/share/nvim/runtime/lua/vim/lsp": true
  },
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "markdownlint.config": {
    "no-duplicate-header": false,
    "no-duplicate-heading": false,
    "ul-indent": {
      "indent": 2
    }
  },
  "cSpell.userWords": [
    "Keiyo",
    "Kyuden",
    "Saimata",
    "autopep",
    "funcobj",
    "markdownlint",
    "nvim",
    "pyright",
    "require'nvim",
    "saitama",
    "startify",
    "unibo",
    "unibos",
    "unimported",
    "unstaged",
    "untracked"
  ]
}
