{
  "coc.preferences.currentFunctionSymbolAutoUpdate": false,
  "coc.preferences.enableFloatHighlight": true,
  "coc.preferences.enableMarkdown": true,
  "coc.preferences.colorSupport": true,
  "coc.preferences.extensionUpdateCheck": "daily",
  "coc.source.file.ignoreHidden": false,
  "diagnostic.refreshOnInsertMode": false,
  "diagnostic.infoSign": "כֿ",
  "diagnostic.hintSign": "•",
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "⚠",
  "diagnostic.virtualText": true,
  "diagnostic.virtualTextCurrentLineOnly": false,
  "diagnostic.virtualTextPrefix": "◆ ",
  "codeLens.enable": true,
  "suggest.snippetsSupport": true,
  "suggest.enablePreview": true,
  "suggest.detailField": "preview",
  "suggest.triggerCompletionWait": 30,
  "suggest.minTriggerInputLength": 1,
  "suggest.enablePreselect": true,
  "suggest.maxCompleteItemCount": 50,
  "suggest.acceptSuggestionOnCommitCharacter": false,
  "snippets.enableStatusItem": false,
  "diagnostic-languageserver.debug": false,
  "diagnostic-languageserver.enable": true,
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "markdown": "markdownlint",
    "go": "golangci-lint"
  },
  "diagnostic-languageserver.linters": {
    "golangci-lint": {
      "command": "golangci-lint",
      "rootPatterns": [".git", "go.mod"],
      "debounce": 1000,
      "args": ["run", "--out-format", "json", "--fast"],
      "sourceName": "golangci-lint",
      "parseJson": {
        "sourceName": "Pos.Filename",
        "sourceNameFilter": true,
        "errorsRoot": "Issues",
        "line": "Pos.Line",
        "column": "Pos.Column",
        "message": "${Text} [${FromLinter}]"
      }
    }
  },
  "html.filetypes": ["html", "handlebars", "html.handlebars"],
  "html.format.enable": false,
  "python.formatting.provider": "autopep8",
  "eslint.autoFixOnSave": true,
  "flutter.lsp.initialization.closingLabels": true,
  "flutter.lsp.initialization.suggestFromUnimportedLibraries": true,
  "flutter.provider.hot-reload": true,
  "translator.toLang": "ja",
  "translator.engines": ["bing", "google"],
  "yaml.validate": false,
  "yaml.completion": true,
  "yaml.format.enable": true,
  "go.goplsUseDaemon": true,
  "go.checkForUpdates": "ask",
  "Lua.workspace.library": {
    "/usr/local/share/nvim/runtime/lua": true,
    "/usr/local/share/nvim/runtime/lua/vim/lsp": true
  },
  "cSpell.ignorePaths": [
    "node_modules",
    "**/node_modules",
    "**/node_modules/**",
    "node_modules/**",
    "vscode-extension",
    ".git",
    "*.dll",
    "**/*.dll"
  ],
  "cSpell.showStatus": false,
  "cSpell.spellCheckDelayMs": 1000,
  "markdownlint.config": {
    "no-duplicate-header": false,
    "no-duplicate-heading": false,
    "ul-indent": {
      "indent": 4
    }
  }
}
